{
   "manifest_version": 2,
   "name": "PauseOnEnd",
   "description": "Pauses the video on its last second",
   "version": "1.6.0",
   "content_scripts": [ {
      "js": [ "PauseOnEnd.js" ],
      "matches": [ "*://www.youtube.com/*" ]
   } ],
   "permissions": [
      "*://www.youtube.com/*", "tabs", "storage"
   ], 
   "icons": {
      "128": "PauseOnEnd.png"
   }
}
